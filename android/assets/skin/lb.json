		file: qs-60-bold.fnt
	}
	qs-60-boldglow: {
		file: qs-60-boldglow.fnt
	}
}
Color: {
	black: {
		r: 0
		g: 0
		b: 0
		a: 1
	}
	disabled: {
		r: 0.19607843
		g: 0.19607843
		b: 0.19607843
		a: 1
	}
	emphasize: {
		r: 1
		g: 0.3137255
		b: 0.3137255
		a: 1
	}
	lightselection: {
		r: 0.25666666
		g: 0.6800357
		b: 1
		a: 1
	}
	unselected: {
		r: 0.47843137
		g: 0.47843137
		b: 0.47843137
		a: 1
	}
	white: {
		r: 1
		g: 1
		b: 1
		a: 1
	}
}
TintedDrawable: {
	slider-knob-disabled: {
		name: slider-knob
		color: disabled
	}
	selection: {
		name: white
		color: lightselection
	}
	smoke: {
		name: white
		color: {
			r: 0
			g: 0
			b: 0
			a: 0.8627451
		}
	}
	black: {
		name: white
		color: black
	}
	transparent: {
		name: white
		color: {
			r: 1
			g: 1
			b: 1
			a: 0
		}
	}
	button_small_font_disabled: {
		name: button_small_font
		color: disabled
	}
	button_rotateright_pressed: {
		name: button_rotateright
		color: disabled
	}
	button_rotateleft_pressed: {
		name: button_rotateleft
		color: disabled
	}
	button_harddrop_pressed: {
		name: button_harddrop
		color: disabled
	}
	whitesmoke: {
		name: white
		color: {
			r: 1
			g: 1
			b: 1
			a: 0.19607843
		}
	}
