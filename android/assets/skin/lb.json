		name: window-bl
		color: lightselection
	}
	button_onscreen_pressed: {
		name: button_onscreen
		color: disabled
	}
}
ButtonStyle: {
	round: {
		up: button_small_font
		down: button_small_font_filled
		over: button_small_font_glow
	}
	smoke: {
		down: whitesmoke
	}
	rotateright: {
		up: button_rotateright
		down: button_rotateright_pressed
	}
	harddrop: {
		up: button_harddrop
		down: button_harddrop_pressed
	}
	rotateleft: {
		up: button_rotateleft
		down: button_rotateleft_pressed
	}
}
LabelStyle: {
	default: {
		font: qs-25-reg
	}
	big: {
		font: qs-25-bold
	}
	bigbigoutline: {
		font: qs-40-bold
	}
	fontawesome: {
		font: fa45
	}
	qs60: {
		font: qs-60-bold
	}
	qs60glow: {
		font: qs-60-boldglow
	}
	qs40glow: {
		font: qs-40-boldglow
	}
