		down: button_small_font_filled
		over: button_small_font_glow
		disabled: button_small_font_disabled
	}
	checkbox: {
		font: qs-40-bold
		fontColor: white
		downFontColor: emphasize
		disabledFontColor: disabled
	}
	fontawesome: {
		font: fa45
		fontColor: white
		downFontColor: emphasize
		disabledFontColor: disabled
	}
	welcome: {
		font: qs-25-reg
		fontColor: white
		downFontColor: emphasize
		disabledFontColor: disabled
	}
	big: {
		font: qs-25-bold
		fontColor: white
		downFontColor: emphasize
		disabledFontColor: disabled
	}
	gamepad: {
		font: qs-40-bold
		fontColor: white
		downFontColor: disabled
		up: button_onscreen
		down: button_onscreen_pressed
	}
}
TextFieldStyle: {
	default: {
		font: qs-40-bold
		fontColor: white
		disabledFontColor: unselected
		background: window
		focusedBackground: focused-window-bl
		cursor: white
		selection: whitesmoke
		messageFont: qs-40-boldglow
