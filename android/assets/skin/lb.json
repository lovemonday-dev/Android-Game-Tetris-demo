		fontColorUnselected: unselected
		selection: transparent
	}
}
ProgressBarStyle: {
	default-horizontal: {
		background: slider
		knobBefore: slider-before
	}
}
ScrollPaneStyle: {
	default: {
		hScrollKnob: scroll-horizontal
		vScrollKnob: scroll-vertical
	}
	pager: {
		hScroll: radio
		hScrollKnob: radio-over
	}
	list: {
		background: list
		hScrollKnob: scroll-horizontal
		vScrollKnob: scroll-vertical
	}
}
SliderStyle: {
	default-horizontal: {
		knobOver: slider-knob-pressed
		background: slider
		knob: slider-knob
		disabledKnob: slider-knob-disabled
		knobBefore: slider-before
	}
}
TextButtonStyle: {
	default: {
		font: qs-40-bold
		fontColor: white
		downFontColor: emphasize
		disabledFontColor: disabled
	}
	round: {
		font: qs-40-bold
