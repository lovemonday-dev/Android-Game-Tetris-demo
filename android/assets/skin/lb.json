		file: fa-45.fnt
	}
	qs-25-bold: {
		file: qs-25-bold.fnt
	}
	qs-25-reg: {
		file: qs-25-reg.fnt
	}
	qs-40-bold: {
		file: qs-40-bold.fnt
	}
	qs-40-boldglow: {
		file: qs-40-boldglow.fnt
	}
