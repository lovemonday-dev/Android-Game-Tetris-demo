	}
}
TextTooltipStyle: {
	default: {
		label: default
	}
}
TouchpadStyle: {
	default: {
		background: touchpad
		knob: touchpad_knob
	}
	dpad: {
		background: dpad
	}
}
WindowStyle: {
	default: {
		background: window-bl
		titleFont: qs-25-bold
		stageBackground: smoke
	}
	overlay: {
		background: window
		titleFont: qs-25-bold
	}
	frameless: {
		titleFont: qs-25-bold
	}
	fullsize: {
		titleFont: qs-25-bold
		stageBackground: black
	}
}
}